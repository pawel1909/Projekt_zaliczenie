<UserControl x:Class="Projekt_zaliczenie.Pages.Start"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Projekt_zaliczenie.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="576" d:DesignWidth="700">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>

        

        <Border Grid.Row="0" Grid.Column="0" CornerRadius="30" Margin="100" Background="Gray">
            <TextBlock x:Name="Nazwa" Margin="20px" FontSize="26" FontWeight="Bold" TextAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center" ></TextBlock>
        </Border>

    </Grid>
</UserControl>
